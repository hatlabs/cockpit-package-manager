    "@vitest/ui": "^4.0.7",
    "dotenv": "^17.2.3",
    "esbuild": "^0.24.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.7"
